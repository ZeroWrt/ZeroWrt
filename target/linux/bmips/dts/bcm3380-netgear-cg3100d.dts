#include "bcm3380.dtsi"

/ {
	model = "Netgear CG3100D";
	compatible = "netgear,cg3100d", "brcm,bcm3380";
};

&uart0 {
	status = "okay";
};

&lsspi {
	status = "okay";
	flash@0 {
		compatible = "jedec,spi-nor";
		spi-max-frequency = <1000000>;
		spi-tx-bus-width = <1>;
		spi-rx-bus-width = <1>;
		reg = <0>;

		#address-cells = <1>;
		#size-cells = <1>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "stock";
				reg = <0x000000 0x800000>;
				read-only;
			};
		};
	};
};
