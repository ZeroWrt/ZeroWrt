&en7581_pinctrl {
	gpio-ranges = <&en7581_pinctrl 0 13 47>; // TODO: fix this

	mdio_pins: mdio-pins {
		mux {
			function = "mdio";
			groups = "mdio";
		};

		conf {
			pins = "gpio2";
			output-high;
		};
	};

	pcie0_rst_pins: pcie0-rst-pins {
		conf {
			pins = "pcie_reset0";
			drive-open-drain = <1>;
		};
	};

	pcie1_rst_pins: pcie1-rst-pins {
		conf {
			pins = "pcie_reset1";
			drive-open-drain = <1>;
		};
	};

	gswp1_led0_pins: gswp1-led0-pins {
		mux {
			function = "phy1_led0";
			pins = "gpio33";
		};
	};

	gswp2_led0_pins: gswp2-led0-pins {
		mux {
			function = "phy2_led0";
			pins = "gpio34";
		};
	};

	pwm_gpio17_idx9_pins: pwm-gpio17-idx9-pins {
		function = "pwm";
		pins = "gpio17";
		output-enable;
	};
};

&snfi {
	status = "okay";
};

&spi_nand {
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <1>;

		partition@0 {
			label = "bootloader";
			reg = <0x00000000 0x00200000>;
			read-only;
		};

		partition@200000 {
			label = "uenv";
			reg = <0x00200000 0x00200000>;
		};

		partition@400000 {
			label = "dsd";
			reg = <0x00400000 0x00200000>;
			read-only;

			nvmem-layout {
				compatible = "ascii-eq-delim-env";
				#address-cells = <1>;
				#size-cells = <1>;

				lan_mac: lan_mac {
					compatible = "mac-base";
					#nvmem-cell-cells = <1>;
				};

				wan_mac: wan_mac {
					compatible = "mac-base";
					#nvmem-cell-cells = <1>;
				};
			};

			nvmem-eeprom {
				compatible = "fixed-layout";
				#address-cells = <1>;
				#size-cells = <1>;

				eeprom_factory_5000: eeprom@5000 {
					reg = <0x5000 0x1e00>;
				};
			};
		};

		partition@600000 {
			label = "kernel";
			reg = <0x00600000 0x01000000>;
		};

		partition@1600000 {
			label = "rootfs";
			reg = <0x01600000 0x1e800000>;
			compatible = "linux,ubi";
		};

		reserved_bmt@1fe00000 {
			label = "reserved_bmt";
			reg = <0x1fe00000 0x00200000>;
			read-only;
		};
	};
};

&i2c0 {
	status = "okay";
};

&pcie0 {
	status = "okay";

	pinctrl-names = "default";
	pinctrl-0 = <&pcie0_rst_pins>;

	pcie@0,0 {
		reg = <0x0000 0 0 0 0>;

		mt7996@0,0 {
			reg = <0x0000 0 0 0 0>;
			nvmem-cells = <&eeprom_factory_5000>;
			nvmem-cell-names = "eeprom";
			ieee80211-freq-limit = <2400000 2500000>, <5170000 5835000>, <5945000 7125000>;

			band@0 {
				/* 2.4 GHz */
				reg = <0>;
				nvmem-cells = <&lan_mac 1>;
				nvmem-cell-names = "mac-address";
			};

			band@1 {
				/* 5 GHz */
				reg = <1>;
				nvmem-cells = <&lan_mac 2>;
				nvmem-cell-names = "mac-address";
			};

			band@2 {
				/* 6 GHz */
				reg = <2>;
				nvmem-cells = <&lan_mac 3>;
				nvmem-cell-names = "mac-address";
			};
		};
	};
};

