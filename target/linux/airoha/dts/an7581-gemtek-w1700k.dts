/dts-v1/;

/memreserve/ 0x80000000 0x200000; // ATF

#include <dt-bindings/leds/common.h>
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include "an7581.dtsi"

/ {
	model = "Gemtek W1700K";
	compatible = "gemtek,w1700k", "airoha,an7581", "airoha,en7581";

	aliases {
		serial0 = &uart1;
		led-boot = &led_red;
		led-failsafe = &led_blue;
		led-upgrade = &led_blue;
		led-running = &led_green;
	};

	chosen {
		bootargs = "console=ttyS0,115200 earlycon";
		stdout-path = "serial0:115200n8";
		linux,usable-memory-range = <0x0 0x80200000 0x0 0x1fe00000>;
	};

	memory@80000000 {
		device_type = "memory";
		reg = < 0x0 0x80000000 0x0 0x80000000>;
	};

	keys {
		compatible = "gpio-keys";

		key-restart {
			label = "Reset";
			gpios = <&en7581_pinctrl 0 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_green: led@0 {
			color = <LED_COLOR_ID_GREEN>;
			gpios = <&en7581_pinctrl 17 GPIO_ACTIVE_LOW>;
			function = LED_FUNCTION_POWER;
			default-state = "on";
		};

		led_blue: led@1 {
			color = <LED_COLOR_ID_BLUE>;
			gpios = <&en7581_pinctrl 19 GPIO_ACTIVE_LOW>;
			function = LED_FUNCTION_STATUS;
		};

		led_white: led@4 {
			color = <LED_COLOR_ID_WHITE>;
			gpios = <&en7581_pinctrl 20 GPIO_ACTIVE_LOW>;
			function = LED_FUNCTION_INDICATOR;
		};

		led_red: led@3 {
			color = <LED_COLOR_ID_RED>;
			gpios = <&en7581_pinctrl 29 GPIO_ACTIVE_LOW>;
			function = LED_FUNCTION_STATUS;
		};
	};

	/*
	fan: pwm-fan {
		compatible = "pwm-fan";
		cooling-levels = <0 25 95 145 195 255>;
		fan-supply = <&vmmc_3v3>
		pinctrl-names = "default";
